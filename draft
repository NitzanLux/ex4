    public static void main(String[] args) {
        Random random=new Random();
        AvlTree avlTree=new AvlTree();
        int size=0;
        for (int i = 0; i <10; i++) {
            avlTree.add(i);
        }
        for (int i = 0; i <10; i++) {
            if (avlTree.contains(i)!=-1){
                size++;
            }
        }
        for (int i = 0; i <10 ; i++) {
            avlTree.delete(i);
        }
        for (int i = 0; i <1000; i++) {
            if (avlTree.contains(i)!=-1){
                size--;
            }
        }
        System.out.println("ffffff");
    }